   public class Program
   {
     public static void Main(string[] args)
     {
       string[] Email = {"raivenblando@gmail.com","blando_raiven@gmail.com","ven073001@gmail.com" };
       
       string[] Password = {"Raiven0123", "Blandoraiven", "Hulaanmopassko"};
       
       string[] Username = {"RaivenBlando", "RaivenB", "Ven Blando"};
       
